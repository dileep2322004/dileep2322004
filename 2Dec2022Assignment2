WAY-1:
def vote(age):
 if age>=18:
  print('eligible for vote')
 else:
  print('not eligible for vote')
age=int(input('enter age'))
vote(age)

WAY-2:
def vote(age):
 if age>=18:
  return 'eligible for vote'
 else:
  return 'not eligible for vote'
age=int(input('enter age'))
print(vote(age))

WAY-3:
 def vote(age):
 if age>=18:
  return True
 else:
  return False
age=int(input('enter age'))
if(vote(age)):
 print('eligible for vote')
else:
 print('not eligible for vote')
