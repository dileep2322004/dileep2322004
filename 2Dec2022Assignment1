WAY-1:
def biggest(a,b):
 if a>b:
  print('a is big')
 else:
  print('b is big')
a=int(input('enter a'))
b=int(input('enter b'))
biggest(a,b)


WAY-2:
def biggest(a,b):
 if a>b:
  return 'a is big'
 else:
  return 'b is big'
a=int(input('enter a'))
b=int(input('enter b'))
print(biggest(a,b))


WAY-3:
def biggest(a,b):
 if a>b:
  return True
 else:
  return False
a=int(input('enter a'))
b=int(input('enter b'))
if (biggest(a,b)):
 print('a is big')
else:
 print('b is big')

